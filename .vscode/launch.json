  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "--dbname",
        "sec",
        "--host",
        "host.docker.internal",
        "--user",
        "secapp",
        "--password",
        "1234",
        "--port",
        "5433"
